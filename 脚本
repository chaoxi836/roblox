  function autobuyv2(o)

   
	local  item=nil
	local  ltem
	
	item=finditem(o)

	if item==nil then
		Notification:Notify( 
     {Title = "蓝", Description = "没有找到商品 请您等待"}, 
     {OutlineColor = Color3.fromRGB(80, 80, 80),Time = 5, Type = "image"}, 
     {Image = "http://www.roblox.com/asset/?id=4483345998", ImageColor = Color3.fromRGB(255, 84, 84)} 
 )
		repeat
